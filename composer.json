{
  "name": "it-can/laravel-helpers",
  "type": "library",
  "description": "My custom Laravel Helpers I use in every Laravel project",
  "keywords": [
    "laravel",
    "helper"
  ],
  "homepage": "https://github.com/it-can/laravel-helpers",
  "license": "MIT",
  "authors": [
    {
      "name": "Michiel Vugteveen",
      "email": "info@it-can.nl"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "illuminate/support": "5.6.*|5.7.*|5.8.*|^6.0",
    "layershifter/tld-extract": "^2.0"
  },
  "require-dev": {
    "erusev/parsedown": "^1.7",
    "laravel/framework": "5.6.*|5.7.*|5.8.*|^6.0",
    "laravelcollective/html": "5.6.*|5.7.*|5.8.*",
    "nesbot/carbon": "^1.26.3 || ^2.0",
    "ramsey/uuid": "^3.7",
    "phpunit/phpunit": "^7.5",
    "mockery/mockery": "^1.0",
    "illuminated/testing-tools": "5.7.*|5.8.*"
  },
  "autoload": {
    "psr-4": {
      "ITCAN\\LaravelHelpers\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ITCAN\\LaravelHelpers\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ITCAN\\LaravelHelpers\\GlobalHelpersServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
